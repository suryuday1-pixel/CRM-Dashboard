{
  "manifest_version": 3,
  "name": "Salesforce CRM Chrome Extension",
  "version": "1.0",
  "description": "Bulk Salesforce Lead Manager",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "activeTab", "storage", "scripting" ]
}
